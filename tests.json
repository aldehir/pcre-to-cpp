{
    "patterns": [
        {
            "name": "gpt2",
            "pattern": "'s|'t|'re|'ve|'m|'ll|'d| ?\\p{L}+| ?\\p{N}+| ?[^\\s\\p{L}\\p{N}]+|\\s+(?!\\S)",
            "tests": [
                "Hello World",
                "I'm going to the store",
                "The year is 2024",
                "Hello, World!",
                "  trailing spaces  ",
                "Multiple   spaces",
                "It's a test",
                "123 numbers 456"
            ]
        },
        {
            "name": "tekken",
            "pattern": "[^\\r\\n\\p{L}\\p{N}]?[\\p{Lu}\\p{Lt}\\p{Lm}\\p{Lo}\\p{M}]*[\\p{Ll}\\p{Lm}\\p{Lo}\\p{M}]+|[^\\r\\n\\p{L}\\p{N}]?[\\p{Lu}\\p{Lt}\\p{Lm}\\p{Lo}\\p{M}]+[\\p{Ll}\\p{Lm}\\p{Lo}\\p{M}]*|\\p{N}| ?[^\\s\\p{L}\\p{N}]+[\\r\\n/]*|\\s*[\\r\\n]+|\\s+(?!\\S)|\\s+",
            "tests": [
                "Hello World",
                "I'm going to the store",
                "The year is 2024",
                "Hello, World!",
                "  trailing spaces  ",
                "Multiple   spaces",
                "It's a test",
                "123 numbers 456"
            ]
        },
        {
            "name": "llama3",
            "pattern": "(?i:'s|'t|'re|'ve|'m|'ll|'d)|[^\\r\\n\\p{L}\\p{N}]?\\p{L}+|\\p{N}{1,3}| ?[^\\s\\p{L}\\p{N}]+[\\r\\n]*|\\s*[\\r\\n]+|\\s+(?!\\S)|\\s+",
            "tests": [
                "Hello World",
                "I'm going to the store. She's been there.",
                "The year is 2024 and pi is 3.14159",
                "Hello123World",
                "Hello, World! How are you?",
                "Multiple   spaces",
                "Line1\nLine2\n\nLine4",
                "I'll I've I'd I'm you're they've we'd she's",
                "UPPERCASE contractions I'M YOU'RE"
            ]
        }
    ]
}
